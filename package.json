{
  "name": "loglevel-datadog",
  "version": "0.0.0-development",
  "description": "loglevel plugin to send logs from the browser to Datadog via the datadog browser logs SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["/dist"],
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release",
    "lint": "npx @biomejs/biome check",
    "lint:fix": "npx @biomejs/biome check --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manbearwiz/loglevel-datadog.git"
  },
  "keywords": [
    "browser",
    "datadog",
    "error",
    "log",
    "logger",
    "logging",
    "loglevel-plugin",
    "loglevel",
    "logs",
    "plugin",
    "typescript"
  ],
  "author": "Kevin Brey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manbearwiz/loglevel-datadog/issues"
  },
  "homepage": "https://github.com/manbearwiz/loglevel-datadog#readme",
  "dependencies": {
    "@datadog/browser-logs": "^4.0.0 || ^5.0.0",
    "loglevel": "^1.8.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^16.7.13",
    "lefthook": "^1.10.5",
    "semantic-release": "^20.1.3",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,css,md}": "prettier --write"
  },
  "release": {
    "branches": ["main"]
  },
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  }
}
